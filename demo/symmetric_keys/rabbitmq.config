[{rabbit,
    [{auth_backends, [rabbit_auth_backend_oauth2, rabbit_auth_backend_internal]}]},
 {rabbitmq_auth_backend_uaa,
    [{resource_server_id, <<"rabbitmq">>}]},
 {uaa_jwt, [
  {default_key, <<"legacy-token-key">>},
  {signing_keys,
    #{
        <<"legacy-token-key">> =>
            {map,
                #{<<"kty">> => <<"MAC">>,
                  <<"value">> => <<"rabbit_signing_key">>,
                  <<"alg">> => <<"HS256">>}
            }
    }
  }]
}].
